<%= form_for(frame) do |f| %>
  <div class="form-group">
    <%= f.label :name, "名前" %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :content, "説明" %>
    <%= f.text_area :content, rows: 15, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :latitude, "緯度" %>
    <%= f.text_field :latitude, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :longitude, "経度" %>
    <%= f.text_field :longitude, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :image, "画像ファイル名" %>
    <%= f.text_field :image, class: "form-control" %>
  </div>
  <div class="text-center">
    <%= f.submit '登録する', class: 'btn btn-success' %>
  </div>
<% end %>